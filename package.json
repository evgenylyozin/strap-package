{
  "name": "strap-package",
  "version": "0.0.0",
  "description": "Strap files and tools to start developing an npm package",
  "bin": {
    "strap-package": "./dist/index.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evgenylyozin/strap-package.git"
  },
  "scripts": {
    "build": "vite build && chmod +x ./dist/index.js",
    "test": "vitest run",
    "lint": "eslint ./src",
    "prettify": "prettier . --write",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "stage-updated": "git add -u",
    "prepare-release": "npm run typecheck && npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "npm",
    "package",
    "template",
    "boilerplate",
    "npx"
  ],
  "author": "Evgenii Evgenevich Lezin (https://github.com/evgenylyozin)",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.10.2",
    "eslint": "^9.13.0",
    "husky": "^9.1.6",
    "prettier": "3.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.9",
    "vitest": "^2.1.3"
  }
}
